{
	"manifest_version":3,
	"name":"Mortality",
	"version":"0.0.1",
	"permissions":[
		"storage",
		"alarms",
		"notifications",
		"activeTab",
		"tabs"
	],
	"background":{
		"service_worker":"background.js"
	},
	"icons":{
		"16": "/images/m16.png",
		"32": "/images/m32.png",
		"48": "/images/m48.png",
		"128": "/images/m128.png"
	},
	"action":{
		"default_title":"Toggle Mortality",
		"default_popup":"popup.html"
	}	
}